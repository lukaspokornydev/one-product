{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {}
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "product-rating": {
          "type": "rating",
          "settings": {}
        },
        "price": {
          "type": "price",
          "settings": {}
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {}
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "description": {
          "type": "description",
          "settings": {}
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "71e8332f-5f91-48be-acf5-54e44f268d37": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Shipping",
            "icon": "truck",
            "content": "<p>Ships in {{ product.metafields.my_fields.shipping.value }}.<\/p>",
            "page": ""
          }
        },
        "78ad3c72-c675-41fd-8659-83fc1a3c7629": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Details",
            "icon": "question_mark",
            "content": "<p>{{ product.metafields.my_fields.origin.value }}{{ product.metafields.my_fields.satisfaction.value }}<\/p>",
            "page": ""
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "product-rating",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "share",
        "71e8332f-5f91-48be-acf5-54e44f268d37",
        "78ad3c72-c675-41fd-8659-83fc1a3c7629"
      ],
      "settings": {
        "enable_sticky_info": true,
        "gallery_layout": "thumbnail",
        "media_size": "medium",
        "mobile_thumbnails": "hide",
        "hide_variants": false,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "YOU MAY ALSO LIKE",
        "heading_size": "h1",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "portrait",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 0,
        "padding_bottom": 0
      }
    },
    "newsletter": {
      "type": "newsletter",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "SUBSCRIBE TO OUR EMAILS",
            "heading_size": "h1"
          }
        },
        "paragraph": {
          "type": "paragraph",
          "settings": {
            "text": "<p>Be the first to know about new collections and exclusive offers.<\/p>"
          }
        },
        "email-form": {
          "type": "email_form",
          "settings": {}
        }
      },
      "block_order": [
        "heading",
        "paragraph",
        "email-form"
      ],
      "settings": {
        "color_scheme": "accent-1",
        "full_width": true,
        "padding_top": 40,
        "padding_bottom": 40
      }
    }
  },
  "order": [
    "main",
    "product-recommendations",
    "newsletter"
  ]
}